{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "agentic-signal",
  "version": "1.1.0",
  "identifier": "com.agentic-signal",
  "build": {
    "frontendDist": "../client/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run client:dev",
    "beforeBuildCommand": "bun run client:build"
  },
  "app": {
    "windows": [
      {
        "title": "Agentic Signal",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "bin/"
    ],
    "publisher": "Code Forge Temple"
  },
  "mainBinaryName": "Agentic Signal"
}